{
  "name": "yournotes",
  "version": "0.1.0",
  "author": "Anton Zimnitski",
  "description": "A note taking app for Mac, Windows and Linux.",
  "main": "public/main.js",
  "homepage": "https://github.com/Zimniros/YourNotes#readme",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zimniros/YourNotes.git"
  },
  "bugs": {
    "url": "https://github.com/Zimniros/YourNotes/issues"
  },
  "keywords": [
    "windows",
    "linux",
    "react",
    "electron"
  ],
  "dependencies": {
    "@craco/craco": "^3.6.0",
    "@mdi/js": "^3.5.95",
    "@mdi/react": "^1.1.0",
    "date-fns": "^1.30.1",
    "electron-is-dev": "^1.0.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.4",
    "react-modal": "^3.8.1",
    "react-quill": "^1.3.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "eject": "craco eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/main.js",
    "preelectron-pack": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "spectron": "^5.0.0",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.yournotes",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
